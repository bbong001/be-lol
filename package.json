    "class-validator": "^0.14.2",
    "dotenv": "^16.5.0",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.14.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
